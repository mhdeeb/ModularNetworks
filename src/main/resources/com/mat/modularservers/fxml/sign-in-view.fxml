<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="218.0" prefWidth="363.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mat.modularservers.controller.SignInController">
   <BorderPane layoutX="17.0" layoutY="31.0" prefHeight="125.0" prefWidth="329.0">
      <center>
         <VBox prefHeight="101.0" prefWidth="258.0" BorderPane.alignment="CENTER">
            <HBox prefHeight="100.0" prefWidth="226.0">
               <Label prefHeight="25.0" prefWidth="70.0" text="username:" />
               <TextField fx:id="username" />
            </HBox>
            <HBox prefHeight="100.0" prefWidth="200.0">
               <Label prefHeight="25.0" prefWidth="70.0" text="password:" />
               <TextField fx:id="password" />
            </HBox>
         </VBox>
      </center>
      <bottom>
         <Button fx:id="signIn" mnemonicParsing="false" text="sign in" BorderPane.alignment="CENTER" />
      </bottom>
      <top>
         <Label fx:id="message" textFill="#c63535" BorderPane.alignment="CENTER">
            <BorderPane.margin>
               <Insets bottom="5.0" />
            </BorderPane.margin>
         </Label>
      </top>
   </BorderPane>
   <Label fx:id="signUp" layoutX="308.0" layoutY="187.0" onMouseEntered="#selectSignUp" onMouseExited="#deselectSignUp" text="Sign up" underline="true">
      <cursor>
         <Cursor fx:constant="HAND" />
      </cursor></Label>
   <CheckBox fx:id="remember" layoutX="112.0" layoutY="169.0" mnemonicParsing="false" text="remember me" />
</AnchorPane>
